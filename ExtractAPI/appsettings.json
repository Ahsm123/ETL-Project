{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "EnableIdempotence": true,
    "Acks": "All",
    "MessageSendMaxRetries": 3,
    "RetryBackoffMs": 100,
    "CompressionType": "Snappy"
  },

  "ConfigService": {
    "BaseUrl": "https://localhost:7027"
  }
}
